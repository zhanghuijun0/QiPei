<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_activity_bg"
    android:orientation="vertical" >
    <FrameLayout
	    android:layout_width="match_parent"
	    android:layout_height="@dimen/common_actionbar_height"
	    android:background="@color/common_actionbar_bg">			
        <TextView
            android:id="@+id/profile_my_favorite_text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            style="@style/Widget.ActionbarTextView"
            android:gravity="center"
            android:text="@string/profile_label_my_favorite_text"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/ic_back"
            android:layout_gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:background="@drawable/companyinfo_lable_back_selector"
            android:onClick="onMyFavoriteBackClick"/>
	</FrameLayout>
	<ScrollView
	    style="@style/Widget.GammaScrollbar"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content">
		<LinearLayout 
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:background="@color/common_activity_bg"
		    android:orientation="vertical" >
		    <TextView
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/profile_gray_line_size"
				android:layout_marginTop="10dp"
		        android:background="@color/common_gray_line_color"/>
		    <RelativeLayout
		        android:id="@+id/profile_my_favorite_vendor_text"
		        android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="15dp"
		        android:background="@drawable/profile_my_relative_layout_selecter"
		        android:clickable="true"
		        android:onClick="onFavoriteVendorClick"
		        >   
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/profile_label_vendor_text"
		            android:layout_alignParentLeft="true"
		            android:textSize="@dimen/profile_first_title_font_size"/>
		        <ImageView
		            android:id="@+id/profile_my_favorite_vendor_small_arrows"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/profile_login_small_arrows"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"/>
		        <TextView
		            android:id="@+id/profile_my_favorite_vendor_num"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginRight="10dp"
		            android:layout_toLeftOf="@id/profile_my_favorite_vendor_small_arrows"
		            android:textSize="@dimen/profile_second_title_font_size"/>
		    </RelativeLayout>
		    <FrameLayout
			    android:layout_width="match_parent"
			    android:layout_height="1dp"
			    android:background="@color/profile_user_info_background">
		        <TextView
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/profile_gray_line_size"
			        android:layout_marginLeft="15dp"
			        android:background="@color/common_gray_line_color"/>
		    </FrameLayout>
			<RelativeLayout
		        android:id="@+id/profile_my_favorite_dealers_text"
		        android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="15dp"
		        android:background="@drawable/profile_my_relative_layout_selecter"
		        android:clickable="true"
		        android:onClick="onFavoriteDealersClick"
		        >  
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/profile_label_dealers_text"
		            android:layout_alignParentLeft="true"
		            android:textSize="@dimen/profile_first_title_font_size"/>
		        <ImageView
		            android:id="@+id/profile_my_favorite_dealers_small_arrows"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/profile_login_small_arrows"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"/>
		        <TextView
		            android:id="@+id/profile_my_favorite_dealers_num"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginRight="10dp"
		            android:layout_toLeftOf="@id/profile_my_favorite_dealers_small_arrows"
		            android:textSize="@dimen/profile_second_title_font_size"/>
		    </RelativeLayout>
			<FrameLayout
			    android:layout_width="match_parent"
			    android:layout_height="1dp"
			    android:background="@color/profile_user_info_background">
		        <TextView
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/profile_gray_line_size"
			        android:layout_marginLeft="15dp"
			        android:background="@color/common_gray_line_color"/>
		    </FrameLayout>
			<RelativeLayout
		        android:id="@+id/profile_my_favorite_terminal_customers_text"
		        android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="15dp"
		        android:background="@drawable/profile_my_relative_layout_selecter"
		        android:clickable="true"
		        android:onClick="onFavoriteTerminalCustomersClick"
		        >  
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/profile_label_terminal_customers_text"
		            android:layout_alignParentLeft="true"
		            android:textSize="@dimen/profile_first_title_font_size"/>
		        <ImageView
		            android:id="@+id/profile_my_favorite_terminal_customers_small_arrows"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/profile_login_small_arrows"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"/>
		        <TextView
		            android:id="@+id/profile_my_favorite_terminal_customers_num"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginRight="10dp"
		            android:layout_toLeftOf="@id/profile_my_favorite_terminal_customers_small_arrows"
		            android:textSize="@dimen/profile_second_title_font_size"/>
		    </RelativeLayout>
		    <TextView
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/profile_gray_line_size"
		        android:background="@color/common_gray_line_color"/>
		    <TextView
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/profile_gray_line_size"
				android:layout_marginTop="25dp"
		        android:background="@color/common_gray_line_color"/>
			<RelativeLayout
		        android:id="@+id/profile_my_favorite_supply_text"
		        android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="15dp"
		        android:background="@drawable/profile_my_relative_layout_selecter"
		        android:clickable="true"
		        android:onClick="onFavoriteSupplyClick"
		        > 
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/profile_label_my_favorite_supply_text"
		            android:layout_alignParentLeft="true"
		            android:textSize="@dimen/profile_first_title_font_size"/>
		        <ImageView
		            android:id="@+id/profile_my_favorite_supply_small_arrows"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/profile_login_small_arrows"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"/>
		        <TextView
		            android:id="@+id/profile_my_favorite_supply_num"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginRight="10dp"
		            android:layout_toLeftOf="@id/profile_my_favorite_supply_small_arrows"
		            android:textSize="@dimen/profile_second_title_font_size"/>
		    </RelativeLayout>
		    <FrameLayout
			    android:layout_width="match_parent"
			    android:layout_height="1dp"
			    android:background="@color/profile_user_info_background">
		        <TextView
			        android:layout_width="match_parent"
			        android:layout_height="@dimen/profile_gray_line_size"
			        android:layout_marginLeft="15dp"
			        android:background="@color/common_gray_line_color"/>
		    </FrameLayout>
			<RelativeLayout
		        android:id="@+id/profile_my_favorite_demand_text"
		        android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="15dp"
		        android:background="@drawable/profile_my_relative_layout_selecter"
		        android:clickable="true"
		        android:onClick="onFavoriteDemandClick"
		        >  
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content"
		            android:text="@string/profile_label_my_favorite_demand_text"
		            android:layout_alignParentLeft="true"
		            android:textSize="@dimen/profile_first_title_font_size"/>
		        <ImageView
		            android:id="@+id/profile_my_favorite_demand_small_arrows"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:src="@drawable/profile_login_small_arrows"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"/>
		        <TextView
		            android:id="@+id/profile_my_favorite_demand_num"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_marginRight="10dp"
		            android:layout_toLeftOf="@id/profile_my_favorite_demand_small_arrows"
		            android:textSize="@dimen/profile_second_title_font_size"/>
		    </RelativeLayout>
		    <TextView
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/profile_gray_line_size"
		       	android:layout_marginBottom="20dp"
		        android:background="@color/common_gray_line_color"/>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
